<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/imageButtonfocooff"
        android:layout_width="177dp"
        android:layout_height="179dp"
        android:adjustViewBounds="true"
        android:contentDescription="@string/IB"
        android:padding="20dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/focooff" />

    <ImageButton
        android:id="@+id/imageButtonemergency"
        android:layout_width="219dp"
        android:layout_height="205dp"
        android:adjustViewBounds="true"
        android:contentDescription="@string/IB2"
        android:padding="20dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonfocooff"
        app:srcCompat="@drawable/emergencia" />

    <ImageButton
        android:id="@+id/imageButtonpinlocation"
        android:layout_width="173dp"
        android:layout_height="179dp"
        android:adjustViewBounds="true"
        android:contentDescription="@string/IB3"
        android:padding="20dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonemergency"
        app:srcCompat="@drawable/pinlocation" />
</androidx.constraintlayout.widget.ConstraintLayout>
